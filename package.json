{
  "name": "sirius",
  "version": "1.0.0",
  "description": "基础功能demo",
  "main": "",
  "scripts": {
    "server": "webpack-dev-server --config ./build/webpack.server.js --open",
    "build:dev": "webpack --config ./build/webpack.config.js --progress --color",
    "build:test": "NODE_ENV=test webpack --config ./build/webpack.config.js",
    "build:release": "NODE_ENV=release webpack --config ./build/webpack.config.js",
    "lint:create": "eslint --init",
    "lint": "eslint src",
    "prettier": "prettier --write src",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenlogin/sirius.git"
  },
  "author": "meng",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --ext .js --fix",
      "git add"
    ],
    "**/*.vue": "eslint --ext .vue"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "bugs": {
    "url": "https://github.com/chenlogin/sirius/issues"
  },
  "homepage": "https://github.com/chenlogin/sirius#readme",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "ali-oss": "^6.16.0",
    "art-template": "^4.13.2",
    "babel-loader": "^8.2.1",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "^1.4.5",
    "escodegen": "^2.0.0",
    "esprima": "^4.0.1",
    "estraverse": "^5.3.0",
    "express": "^4.17.1",
    "express-art-template": "^1.0.1",
    "express-http-proxy": "^1.6.2",
    "jszip": "^3.6.0",
    "koa": "^2.14.1",
    "koa-router": "^12.0.0",
    "moment": "^2.29.1",
    "react-resizable-rotatable-draggable": "^0.2.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "webpack": "^5.5.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
